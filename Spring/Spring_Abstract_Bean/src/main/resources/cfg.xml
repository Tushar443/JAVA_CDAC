<bean id="sh" class="com.cdac.comp.Shape" abstract="true">
<property name="area" value="4.4"></property>
</bean>

<bean id="rect" class="com.cdac.comp.Rectangular" parent="sh">
	<property name="length" value="1.6"></property>
	<property name="breath" value="3.4"></property>
</bean>
<bean id="rect1" class="com.cdac.comp.Rectangular" parent="sh">
